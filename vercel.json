{
	"github": {
		"silent": true
	},
	"framework": "eleventy",
	"buildCommand": "npm run build",
	"outputDirectory": "_site",
	"routes": [
		{
			"src": "^/$",
			"headers": {
				"accept-language": "ja"
			},
			"status": 302,
			"dest": "/ja"
		},
		{
			"src": "^/$",
			"status": 302,
			"dest": "/zh"
		}
	]
}
